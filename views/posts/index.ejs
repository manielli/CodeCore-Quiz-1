<%- include("./partials/header")%>

<div class="post-cards">
  <% for (let post of posts) { %>
    <div class="card mb-4">
      <div class="card-body">
        <a href="/posts/<%= post.user_id %>">
          <h5 class="card-title"><%= post.id %></h5>
        </a>  
        <p class="card-text"><%= post.content %></p>
      </div>
    </div>
  <% } %>
</div>


<%- include("./partials/footer")%>